{
    "name": "serfe/module-uncacheableblockdetector",
    "description": "Detects blocks that are uncacheable inside your Magento 2 system",
    "type": "magento2-module",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Esteban Zeller",
            "email": "info@serfe.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php":  "7.*|^8.1",
        "magento/framework": "*",
        "magento/module-page-cache": "*",
        "magento/module-store": "*"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Serfe\\UncacheableBlockDetector\\": ""
        }
    },
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    },
    "config": {
      "process-timeout": 0
    }
}
